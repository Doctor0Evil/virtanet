            this.state.typesOfPlot = ['Bar chart plot', 'Cumulative frequency plot', 'Dot plot'];
            this.state.gridData = this.defaultGridData;
        },
        getStoriesByMetric(entry) {
            const stories = [];
            for (const e of this.sampleArr) {
